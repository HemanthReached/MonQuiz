<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#8fFFFFFF"
                android:centerColor="#8fFFFFFF"
                android:centerY="0.75"
                android:endColor="#8fFFFFFF"
                android:angle="270" />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#7DC2AA"
                    android:centerColor="#7DC2AA"
                    android:centerY="0.75"
                    android:endColor="#7DC2AA"
                    android:angle="270" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#7DC2AA"
                    android:endColor="#7DC2AA"
                    android:angle="270" />
            </shape>
        </clip>
    </item>
</layer-list>